module SumModule;

routine Sum(a: integer, b: Integer): Integer is
  var result: Integer;
  result := a + b;
  return result;
end

routine Main() is
  var numA, numB, res: Integer;

  write("Enter first number: ");
  readln(numA);
  write("Enter second number: ");
  readln(numB);
  
  var g: Object;

  g.b().d[0].e := 1;
  g.c();

  res := Sum(numA, numB);
  
  writeln("Result is: ", res);
end