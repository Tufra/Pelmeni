routine Sum(var a, b: Integer): Integer is
  var result: Integer;
  result := a + b;
  return result;
end

routine Main() is
  var numA, numB, res: Integer;

  write('Enter first number: ');
  readln(numA);
  write('Enter second number: ');
  readln(numB);

  res := Sum(numA, numB);
  
  writeln('Result is: ', res);
end